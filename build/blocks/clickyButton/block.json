{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "blockylicious/clicky-button",
  "version": "0.1.0",
  "title": "Clicky Button",
  "category": "blockylicious",
  "icon": "smiley",
  "description": "A call to action button that link to a particular post of page rather than hardcoding the destination URL.",
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "text": true,
      "link": false,
      "gradients": true,
      "enableContrastChekcer": true
    },
    "spacing": {
      "padding": true
    }
  },
  "attributes": {
    "postType": {
      "type": "string",
      "default": ""
    },
    "linkedPost": {
      "type": "number",
      "default": null
    },
    "enableCustomLink": {
      "type": "boolean",
      "default": false
    },
    "customLink": {
      "type": "string",
      "default": ""
    },
    "labelText": {
      "type": "string",
      "default": ""
    },
    "style": {
      "type": "object",
      "default": {
        "color": {
          "background": "#000000",
          "text": "#ffffff"
        },
        "spacing": {
          "padding": {
            "top": "10px",
            "bottom": "10px",
            "left": "30px",
            "right": "30px"
          }
        }
      }
    }
  },
  "textdomain": "blockylicious",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "parent": [
    "blockylicious/clicky-group"
  ]
}